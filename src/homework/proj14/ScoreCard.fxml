<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="homework.proj14.ScoreCardController">
   <children>
      <TableView fx:id="tblScoreCard" maxWidth="1.7976931348623157E308" prefHeight="570.0" HBox.hgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="colIndex" editable="false" minWidth="30.0" prefWidth="30.0" resizable="false" sortable="false" text="#" />
          <TableColumn fx:id="colOption" editable="false" minWidth="150.0" prefWidth="150.0" resizable="false" sortable="false" text="Option" />
          <TableColumn fx:id="colHowToScore" editable="false" minWidth="200.0" prefWidth="200.0" resizable="false" sortable="false" text="How To Score" />
            <TableColumn fx:id="colScore" editable="false" minWidth="100.0" prefWidth="100.0" resizable="false" sortable="false" text="Score" />
        </columns>
      </TableView>
   </children>
</HBox>
